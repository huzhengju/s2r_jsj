<!-- 极视角赛题2场景 -->
<mujoco model="jishijiao_s2">
<include file="airbot_play/airbot_play_options.xml"/>
<include file="airbot_play/airbot_play_dependencies.xml"/>


  <visual>
    <global offwidth="1920" offheight="1080"/>
    <quality shadowsize="4096" offsamples="4"/>
    <headlight diffuse='0.6 0.6 0.6' ambient='.3 .3 .3' specular='0 0 0'/>
    <scale com="0.05" camera="0.06" framelength="0.5" framewidth="0.02" forcewidth="0.1" contactwidth="0.01" contactheight="0.05"/>
  </visual>

  <asset>
    <texture type='2d' name='groundplane' builtin='checker' mark='edge' rgb1='0.2 0.3 0.4' rgb2='0.1 0.2 0.3' markrgb='0.8 0.8 0.8' width='300' height='300'/>
    <material name='groundplane' texture='groundplane' texuniform='true' texrepeat='5 5' reflectance='0.2'/>
  </asset>

<include file="object/tuopan_dependencies.xml"/>
<include file="object/code1_dependencies.xml"/>
<include file="object/code2_dependencies.xml"/>
<include file="object/code3_dependencies.xml"/>

<compiler angle="radian" meshdir="../meshes" texturedir="../textures/"/>

<default>
<default class="obj_visual">
<geom group="2" type="mesh" contype="0" conaffinity="0"/>
</default>
<default class="obj_collision">
<geom group="3" condim="4" solimp="2 1 0.01" solref="0.01 1" friction="1 0.005 0.0001" type="mesh"/>
</default>

</default>

<worldbody>
  <light castshadow="false" directional='true' diffuse='.3 .3 .3' specular='0.3 0.3 0.3' pos='-1 -1 2.5' dir='1 1 -1'/>
      <light directional='true' diffuse='.3 .3 .3' specular='0.3 0.3 0.3' pos='1 -1 2.5' dir='-1 1 -1'/>
      <light castshadow="false" directional='true' diffuse='.3 .3 .3' specular='0.3 0.3 0.3' pos='0 1 2.5' dir='0 -1 -1'/>
      <geom name='floor' type='plane' size='0 0 0.01' material='groundplane' pos='0 0 0'/>

      <site name="site_round2_target" pos="-1.0 -0.05 0.75" size="0.005" rgba="0 0 0 1" type="sphere"/>
      <site name="site_round3_target" pos="-1.0  0.15 0.75" size="0.005" rgba="0 0 0 1" type="sphere"/>
    
<body name="arm_pose" pos="0.12 0.80 0.75" euler="0 0 3.15159">
      <include file="airbot_play/airbot_play.xml"/>
    </body>

<body name="tuopan" pos="-0.2 0.80 0.75" euler="0 0 0">
      <include file="object/tuopan.xml"/>
    </body>

<body name="code1" pos="-0.2 0.88 0.85" euler="0 0 0">
      <joint type="free" frictionloss="0.005" />

      <include file="object/code1.xml"/>
    </body>

    <!-- 梯形  -->
<body name="code2" pos="-0.2 0.72 0.80" euler="0 0 0">
      <joint type="free" frictionloss="0.005" />
      <include file="object/code2.xml"/>
    </body>

<body name="code3" pos="-0.2 0.80 0.80" euler="0 0 0">
      <joint type="free" frictionloss="0.005" />

      <include file="object/code3.xml"/>
    </body>


<body name="table" pos="-0.18 0.75 0" euler="0 0 0">
  <geom size="0.4 0.8 0.01" pos="0 0 0.74" euler="0 0 0" type="box" contype="0" conaffinity="0" rgba="1 1 1 1"/>
  <geom size="0.4 0.8 0.05" pos="0 0 0.7" euler="0 0 0" type="box" rgba="1 0 0 1" group="4"/>
  <geom size="0.02 0.02 0.37" pos="0.35 0.75 0.37" type="box" rgba="0.15 0.15 0.15 1" />
  <geom size="0.02 0.02 0.37" pos="-0.35 0.75 0.37" type="box" rgba="0.15 0.15 0.15 1" />
  <geom size="0.02 0.02 0.37" pos="0.35 -0.75 0.37" type="box" rgba="0.15 0.15 0.15 1" />
  <geom size="0.02 0.02 0.37" pos="-0.35 -0.75 0.37" type="box" rgba="0.15 0.15 0.15 1" />
</body>



</worldbody>
<actuator>
<include file="airbot_play/airbot_play_control.xml"/>
</actuator>
<sensor>
<include file="airbot_play/airbot_play_sensor.xml"/>
</sensor>

</mujoco>
